  },
  "extra": {
    "aws/aws-sdk-php": [
      "S3"
    ],
    "bamarni-bin": {
      "bin-links": false,
      "target-directory": "."
    },
    "config-dir": "config",
    "src-dir": "demosplan",
    "symfony": {
      "allow-contrib": true,
      "require": "6.*.*"
    }
  },
  "scripts": {
    "pre-autoload-dump": "Aws\\Script\\Composer\\Composer::removeUnusedServices"
  },
  "config": {
    "process-timeout": 600,
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "demos-europe/demosplan-addon-installer": true,
      "bamarni/composer-bin-plugin": true,
      "php-http/discovery": false,
