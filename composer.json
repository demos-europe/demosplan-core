    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Base\\": "tests/backend/base/",
            "Tests\\Core\\": "tests/backend/core/",
            "Tests\\CoreApplication\\": "tests/backend/core-application/src/",
            "Tests\\Plugin\\": "tests/backend/plugins/",
            "Tests\\Project\\": "tests/backend/projects/"
        }
    },
    "extra": {
      "bamarni-bin": {
        "bin-links": false,
        "target-directory": "."
      },
      "config-dir": "config",
      "src-dir": "demosplan",
      "symfony": {
            "allow-contrib": true,
            "require": "5.*.*"
        }
    },
    "config": {
        "process-timeout": 600,
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "demos-europe/demosplan-addon-installer": true,
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": false
        },
