{% extends '@DemosPlanCore/DemosPlanCore/base_oeb.html.twig' %}

{% block component_part %}
    {% embed '@DemosPlanCore/DemosPlanCore/includes/participation_area_skeleton.html.twig' with {
    'content_heading': title
    }%}

        {% block content %}

            <p>
                Nachfolgende Kommunen / Verfahrensträger, Planungsbüros, sowie beteiligte Behörden und Institutionen arbeiten mit der Online-Beteiligung in der Bauleitplanung und haben ihr Einverständnis zur Veröffentlichung an dieser Stelle erteilt.
            </p>
            {% if templateVars.orgas is defined %}
                <ul>
                    {% for orga in templateVars.orgas %}
                        <li>{{ orga.name }}</li>
                    {% endfor %}
                </ul>
            {% endif %}

        {% endblock content %}

    {% endembed %}

{% endblock %}
