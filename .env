# In all environments, the following files are loaded if they exist,
# the latter taking precedence over the former:
#
#  * .env                contains default values for the environment variables needed by the app
#  * .env.local          uncommitted file with local overrides
#  * .env.$APP_ENV       committed environment-specific defaults
#  * .env.$APP_ENV.local uncommitted environment-specific overrides
#
# Real environment variables win over .env files.
#
# DO NOT DEFINE PRODUCTION SECRETS IN THIS FILE NOR IN ANY OTHER COMMITTED FILES.
# https://symfony.com/doc/current/configuration/secrets.html
#
# Run "composer dump-env prod" to compile .env files for production use (requires symfony/flex >=1.2).
# https://symfony.com/doc/current/best_practices.html#use-environment-variables-for-infrastructure-configuration

###> demos/demosplan ###
ADDON_REPOSITORY_TOKEN=
APP_TEST_SHARD=test
AVSCAN_HOST=clamd
AVSCAN_PORT=3310
AVSCAN_TIMEOUT=30
ELASTICSEARCH_USERNAME=
ELASTICSEARCH_PASSWORD=
GITHUB_TOKEN=
OAUTH_KEYCLOAK_CLIENT_ID=keycloak_id
OAUTH_KEYCLOAK_CLIENT_SECRET=keycloak_secret
OAUTH_KEYCLOAK_AUTH_SERVER_URL=localhost
OAUTH_KEYCLOAK_REALM=main
OAUTH_AZURE_CLIENT_ID=
OAUTH_AZURE_CLIENT_SECRET=
OAUTH_AZURE_URL_LOGIN=
OAUTH_AZURE_PATH_AUTHORIZE=
OAUTH_AZURE_PATH_TOKEN=
OAUTH_AZURE_TENANT=common
PROCEDURE_METRICS_MAIL=
S3_BUCKET=dplan-files
S3_PREFIX=
S3_REGION=europe-west1
S3_ENDPOINT=
S3_USE_PATH_STYLE_ENDPOINT=true
S3_KEY=
S3_SECRET=
#may be local.storage for files on the same system or s3.storage
FILES_SOURCE=local.storage
PROCEDURE_METRICS_MAIL=
# Session storage: demosplan\\DemosPlanCoreBundle\\Logic\\DatabaseSessionHandler (database) or redis://redis:6379/1 (redis via Predis)
SESSION_HANDLER=demosplan\\DemosPlanCoreBundle\\Logic\\DatabaseSessionHandler
# Session lifetime in seconds (28800 = 8 hours)
SESSION_LIFETIME=28800
# Rate limiting configuration
RATELIMIT_API_ENABLE=true
RATELIMIT_JWT_TOKEN_LIMIT=500
RATELIMIT_JWT_TOKEN_INTERVAL="30 days"
RATELIMIT_PUBLIC_STATEMENT_ENABLE=true
RATELIMIT_ANONYMOUS_STATEMENT_LIMIT=4
RATELIMIT_ANONYMOUS_STATEMENT_RATE_INTERVAL="15 minutes"
RATELIMIT_ANONYMOUS_STATEMENT_RATE_AMOUNT=1
# Auto-enable procedure creation permission for planning agencies on installation/customer creation
# Set to 'true' to automatically enable, 'false' to require manual enabling via command
# This enables the 'feature_admin_new_procedure' permission for PLANNING_AGENCY organisations with RMOPSA role
AUTO_ENABLE_PROCEDURE_CREATION=true
