{
  "name": "@init/diplan-karten",
  "version": "0.5.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/diplan-karten.umd.cjs",
  "module": "./dist/diplan-karten.js",
  "types": "./dist/types",
  "exports": {
    ".": {
      "import": "./dist/diplan-karten.js",
      "require": "./dist/diplan-karten.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "publishConfig": {
    "registry": "https://www.dev.diplanung.de/DefaultCollection/DiPlanKarten/_packaging/karten.feed/npm/registry/"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8082",
    "build": "run-s test \"build-only {@}\" --",
    "build-only": "vite build",
    "audit": "npm audit --omit=dev --audit-level=moderate",
    "preview": "vite preview",
    "//test TODO add audit + test-coverage is faulty atm, will be fixed in #26612": "run-s audit prettier:check eslint:check test:ts test:coverage",
    "test": "run-s prettier:check eslint:check test:ts",
    "eslint:check": "eslint .",
    "eslint:write": "eslint . --fix",
    "prettier:check": "prettier --check src/",
    "prettier:write": "prettier --write src/",
    "test:ts": "vue-tsc --noEmit --skipLibCheck --project tsconfig.app.json",
    "test:coverage": "vitest run --coverage",
    "generate-sbom": "mkdirp sbom && npm sbom --sbom-format cyclonedx > sbom/$(echo ${npm_package_name} | sed 's/@[^/]*\\///')-${npm_package_version}-sbom.cdx.json",
    "prepack": "npm run generate-sbom"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@formkit/addons": "1.6.9",
    "@formkit/auto-animate": "0.8.2",
    "@formkit/core": "^1.6.9",
    "@formkit/inputs": "^1.6.9",
    "@formkit/vue": "1.6.9",
    "@masterportal/masterportalapi": "2.45.0",
    "@oruga-ui/oruga-next": "0.10.2",
    "@oruga-ui/theme-bootstrap": "^0.8.2",
    "@turf/turf": "^7.2.0",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "diplanung-style": "^13.1.0",
    "lodash-es": "^4.17.21",
    "ol": "10.3.1",
    "ol-ext": "^4.0.26"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "mkdirp": "^3.0.1",
    "npm-run-all2": "^7.0.2",
    "postcss-inline-svg": "^6.0.0",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "stylelint": "^16.16.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1",
    "vitest-sonar-reporter": "^2.0.0",
    "vue-tsc": "^2.2.8"
  },
  "resolutions": {
    "protobufjs": "7.3.0"
  }
}
